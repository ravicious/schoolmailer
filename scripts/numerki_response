function zmieniaj(r,t,s)
{ 	
  obiekt= document.getElementById("losik"); 
	obiekt.style.fontSize = r + "px";	
	
	obiekt.style.color="#007799";
	if (r>300){s=-2;}	
	if (r<292){s=2;t=500;}
	
	r=r+s;	
  setTimeout("zmieniaj("+r+","+t+","+s+")");
  return false;
}
