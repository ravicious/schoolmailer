 // <![CDATA[
 
 
  function UkryjOpcje(nr)
{ 
  var p="ukrywany"+nr;
	
  if (p.length < 1){return;}
  document.getElementById(p).style.display = "none";
	var p="minus"+nr;	
	document.getElementById(p).style.display = "none";
	var p="plus"+nr;
	document.getElementById(p).style.display = "";
	
}

function OdkryjOpcje(nr,ile)
{
  var p="ukrywany"+nr;
	
  if (p.length < 1){return;}
  document.getElementById(p).style.display = "block";	
	var p="minus"+nr;
	document.getElementById(p).style.display = "";
	var p="plus"+nr;
	document.getElementById(p).style.display = "none";
	for (i=1;i<=ile;i++)
	{
	  if (i!=nr){UkryjOpcje(i);}
	}
}
 
